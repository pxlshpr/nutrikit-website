# SSH Connection to Mac
SSH_HOST=100.x.x.x          # Mac's Tailscale IP
SSH_PORT=22
SSH_USERNAME=your_username
SSH_PRIVATE_KEY_PATH=~/.ssh/id_ed25519

# Server Config
PORT=3001
BIND_ADDRESS=127.0.0.1      # Use 127.0.0.1 for local only, or Tailscale IP

# Optional Basic Auth (leave empty to disable)
AUTH_USERNAME=
AUTH_PASSWORD=
